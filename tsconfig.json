{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
//      "tslib": [
//        "./node_modules/tslib/tslib.d.ts"
//      ]
    },
    "plugins": [
      {
        "name": "@0no-co/graphqlsp",
        "schema": "./graphql/schema/schema.graphql"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
