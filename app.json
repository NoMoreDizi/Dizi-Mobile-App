{
  "expo": {
    "name": "Dizi",
    "slug": "Dizi",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app/DiziAppIcon.png",
    "scheme": "dizi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app/DiziAppIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
